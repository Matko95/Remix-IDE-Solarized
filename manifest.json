{
  "manifest_version": 2,

  "name": "Remix IDE - Solarized Dark Theme",
  "description": "This Extension will provide a dark theme for Remix IDE.",
  "version": "1.0.0",
  "content_scripts": [{
    "css": ["solarized.css"],
    "js": ["content.js"],
    "matches": [
      "https://remix.ethereum.org/*",
      "http://127.0.0.1/*",
      "http://localhost/*"
    ]
  }],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}